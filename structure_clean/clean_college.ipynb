    "def extract_coordinates(df):\n",
    "    \"\"\"\n",
    "    Convertit la colonne 'the_geom' au format WKT POINT(lon lat)\n",
    "    en deux colonnes 'lat' et 'lon' de type float.\n",
    "    \n",
    "    Supprime ensuite la colonne 'the_geom'.\n",
    "    \"\"\"\n",
    "    if \"the_geom\" not in df.columns:\n",
    "        raise ValueError(\"La colonne 'the_geom' n'existe pas dans le DataFrame.\")\n",
    "    \n",
    "    # Extraction des coordonnées\n",
    "    df[[\"lon\", \"lat\"]] = (\n",
    "        df[\"the_geom\"]\n",
    "        .str.replace(r\"POINT\\s*\\(|\\)\", \"\", regex=True)  # Supprime 'POINT(' et ')'\n",
    "        .str.split(\" \", expand=True)                     # Sépare lon et lat\n",
    "        .astype(float)\n",
    "    )\n",
    "    \n",
    "    # Supprimer la colonne d'origine\n",
    "    df.drop(columns=[\"the_geom\"], inplace=True)\n",
    "    \n",
    "    return df\n",
    "\n",
    "df_college = extract_coordinates(df_college)\n",
    "df_lycees = extract_coordinates(df_lycees)\n",
    "df_college.head()\n",
    "\n",
    "df_college.to_csv(\"../csv_clean/colleges.csv\", index=False)\n",
    "df_lycees.to_csv(\"../csv_clean/lycees.csv\", index=False)"
   ]
